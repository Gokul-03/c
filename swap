/******************************************************************************
swapping by value 
*******************************************************************************/

#include <stdio.h>
void swap(int a,int b)
{
    int t=a;
    a=b;
    b=t;
    printf("%d,%d",a,b);
}
void main()
{
    int x,y;
    x=10;
    y=4;
    swap(x,y);
}
